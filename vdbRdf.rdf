         <doco:TextChunck rdf:about="http://vespasianodabisticciletters.unibo.it/tomasi-letter-18-tergo">
            <dcterms:type rdf:resource="http://vespasianodabisticciletters.unibo.it/tergo"/>
            <c4o:hasContent>[a tergo] &lt;Clarissi&gt;mo atque eloquen&lt;tissimo&gt; viro Piero di &lt;Chosimo&gt; de' Medici maiori &lt;suo ho&gt;norando.</c4o:hasContent>
            <frbr:embodiment>
               <fabio:WebManifestation rdf:about="http://vespasianodabisticciletters.unibo.it/tomasi-letter-18-tergo-html">
                  <frbr:exemplar rdf:resource="http://vespasianodabisticciletters.unibo.it/letters/18.html#tomasi-letter-18-tergo"/>
               </fabio:WebManifestation>
            </frbr:embodiment>
            <po:contains>
               <doco:TextChunck rdf:about="http://vespasianodabisticciletters.unibo.it/tomasi-letter-18-l18-pn3">
                  <rdfs:label xml:lang="it">Nota filologica</rdfs:label>
                  <rdfs:label xml:lang="en">Philological note</rdfs:label>
                  <dcterms:type rdf:resource="http://vespasianodabisticciletters.unibo.it/philological-note"/>
                  <dcterms:type rdf:resource="http://vespasianodabisticciletters.unibo.it/integration"/>
                  <c4o:hasContent>&lt;Clarissi&gt;mo atque eloquen&lt;tissimo&gt; viro Piero di &lt;Chosimo&gt; de' Medici maiori &lt;suo ho&gt;norando.</c4o:hasContent>
                  <dcterms:description>Integrazioni per rasura. Già in Cagni con segnalazione di integrazione</dcterms:description>
                  <dcterms:source rdf:resource="http://vespasianodabisticciletters.unibo.it/tomasi-letter-18-tergo"/>
