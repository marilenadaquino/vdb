   <fabio:Letter rdf:about="http://vespasianodabisticciletters.unibo.it/tomasi-letter-2-expr">
      <dcterms:title>2 - Vespasiano a Filippo Podocataro. Firenze, 4 dicembre 1448.</dcterms:title>
      <frbr:part>
         <doco:TextChunck rdf:about="http://vespasianodabisticciletters.unibo.it/tomasi-letter-2-salutatio">
