               <deo:Reference rdf:about="http://vespasianodabisticciletters.unibo.it/note-14ref-to-giustiniani-ed">
                  <dcterms:references rdf:resource="http://vespasianodabisticciletters.unibo.it/giustiniani-ed"/>
               </deo:Reference>
            </po:contains>
