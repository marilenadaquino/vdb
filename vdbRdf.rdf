               <deo:Reference rdf:about="http://vespasianodabisticciletters.unibo.it/note-5ref-to-filelfo-ed">
                  <dcterms:references rdf:resource="http://vespasianodabisticciletters.unibo.it/filelfo-ed"/>
               </deo:Reference>
            </po:contains>
