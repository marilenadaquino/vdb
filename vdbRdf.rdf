               <fabio:WebManifestation rdf:about="http://vespasianodabisticciletters.unibo.it/tomasi-letter-14-maintext-html">
                  <frbr:exemplar rdf:resource="http://vespasianodabisticciletters.unibo.it/letters/14.html#tomasi-letter-14-maintext"/>
               </fabio:WebManifestation>
